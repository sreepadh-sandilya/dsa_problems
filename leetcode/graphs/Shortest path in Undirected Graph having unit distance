#include<unordered_map>
#include<list>
#include<queue>
vector<int> shortestPath( vector<pair<int,int>> edges , int N , int m, int src , int t){
	
	// Write your code here
	unordered_map <int,list<int>>adj;
        for(int i=0;i<edges.size();i++)
        {
            int u=edges[i].first;
            int v=edges[i].second;
            adj[u].push_back(v);
            adj[v].push_back(u);
        }
        unordered_map <int,bool>visited;
        unordered_map <int,int>parent;
        queue<int>q;
        q.push(src);
        visited[src]=true;
        parent[src]=-1;
        while(!q.empty())
        {
            int front=q.front();
            q.pop();
            
            for(auto i:adj[front])
            {
                if(!visited[i])
                {
                    q.push(i);
                    visited[i]=true;
                    parent[i]=front;
                    
                }
            }
            
        }
		// cout<<"puka";
        vector<int>ans;
        int current=t;
		ans.push_back(t);
		// cout<<t<<endl;
		while(current!=src)
		{
			// cout<<current<<endl;
			// cout<<current<<" ";
			current=parent[current];
			ans.push_back(current);

		}
		reverse(ans.begin(),ans.end());
		// for(auto i:ans)
		// {
		// 	cout<<i<<" ";
		// }
        
        return ans;
    
}
