class Solution {
public:
    bool rotateString(string s, string goal) {
        int n=s.length();
        int m=goal.length();
       if(n!=m)
       {
           return false;
       }
       for(int i=0;i<n;i++)
       {
           int y=0;
           for(int j=0;j<n;j++)
           {
             if(s[j]!=goal[(j+i)%n])
             {
                 y=0;
                 break;
             }
             else
             {
                 y=1;
             }
           }
           if(y==1)
           {
               return true;
           }
       }

       return false;
    }
};
